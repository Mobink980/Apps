
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:minWidth="25px"
    android:minHeight="25px"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/linearLayout1" >
    <Button
        android:text="Button"
        android:layout_width="match_parent"
        android:layout_height="38.0dp"
        android:id="@+id/button1" />
    <Button
        android:text="Button"
        android:layout_width="match_parent"
        android:layout_height="32.0dp"
        android:background = "#eedd45"
        android:id="@+id/button2" />
    <EditText
        android:layout_width="match_parent"
        android:layout_height="34.0dp"
        android:id="@+id/editText1" />
    <TextView
        android:text="Text"
        android:layout_width="match_parent"
        android:layout_height="17.0dp"
        android:id="@+id/textView1" />
    <EditText
        android:layout_width="match_parent"
        android:layout_height="35.5dp"
        android:id="@+id/editText2" />
    <ImageView
        android:src="@android:drawable/ic_menu_gallery"
        android:layout_width="match_parent"
        android:layout_height="74.5dp"
        android:id="@+id/imageView1" />
    <SeekBar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/seekBar1" />
    <TextView
        android:text="Text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView2" />
    <android.webkit.WebView
        android:layout_width="match_parent"
        android:layout_height="220.5dp"
        android:id="@+id/webView1" />
    <EditText
        android:inputType="time"
        android:layout_width="match_parent"
        android:layout_height="47.0dp"
        android:id="@+id/editText3" />
</LinearLayout>